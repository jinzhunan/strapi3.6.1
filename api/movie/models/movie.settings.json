{
  "kind": "collectionType",
  "collectionName": "movies",
  "info": {
    "name": "movie",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "videos": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "comments": {
      "collection": "comment",
      "via": "movie"
    },
    "movieType": {
