      "enum": [
        "movie",
        "drama",
        "anime",
        "game"
      ]
    },
    "movie": {
      "type": "enumeration",
      "enum": [
        "comedy",
        "romantic",
        "action",
        "fantasy"
      ]
    },
    "drama": {
      "type": "enumeration",
      "enum": [
        "romantic",
        "history",
        "comedy",
        "family"
      ]
    },
    "anime": {
      "type": "enumeration",
      "enum": [
        "fantasy",
        "action",
        "romantic",
        "school"
      ]
    },
    "game": {
      "type": "enumeration",
      "enum": [
        "whatever"
      ]
    }
